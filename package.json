{
  "name": "@emiljohansson/random-string",
  "version": "2.0.1",
  "description": "Generates a random string [0-9a-zA-Z]",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "prepublish": "tsc",
    "test": "ava"
  },
  "keywords": [
    "random",
    "string"
  ],
  "author": "Emil Johansson <emiljohansson.se@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "4.3.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.8.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
